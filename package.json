{
  "name": "conventional-changelog-beemo",
  "version": "3.0.1",
  "description": "Conventional changelog preset for Beemo and associated projects.",
  "keywords": [
    "conventional-changelog",
    "beemo",
    "preset"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": "https://github.com/beemojs/conventional-changelog-beemo",
  "author": "Miles Johnson",
  "license": "MIT",
  "files": [
    "index.js",
    "lib/**/*",
    "src/**/*",
    "templates/**/*"
  ],
  "scripts": {
    "build": "packemon build --addEngines",
    "pack": "packemon pack --addEngines --declaration",
    "check": "yarn run type && yarn run test",
    "coverage": "yarn run jest --coverage",
    "test": "vitest",
    "format": "prettier --write .",
    "release": "yarn run pack && yarn run check && npx np --yolo --no-yarn",
    "type": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.12.10",
    "@types/shelljs": "^0.8.15",
    "conventional-changelog-core": "^4.2.4",
    "conventional-recommended-bump": "^6.1.0",
    "packemon": "^4.0.1",
    "prettier": "^3.2.5",
    "prettier-config-moon": "^1.1.2",
    "shelljs": "^0.8.5",
    "tsconfig-moon": "^1.3.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "format": "lib",
    "platform": "node"
  },
  "packageManager": "yarn@3.2.0"
}
